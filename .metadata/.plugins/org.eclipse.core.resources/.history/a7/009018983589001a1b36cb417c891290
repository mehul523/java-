package multitreading;


class table {
	  
	 void print(int n) {
		 
		 for(int i=1;i<=5;i++){  
		     System.out.println(n*i);
		     
		     //sleep thread
		     
		     try{  
		         Thread.sleep(400);  
		        }catch(Exception e){System.out.println(e);}  
	 }
}
