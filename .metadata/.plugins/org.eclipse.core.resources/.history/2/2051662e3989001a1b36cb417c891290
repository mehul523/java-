package multitreading;

class table3 {
	   
	  synchronized static void print(int n) {
		     for(int i=1;i<10;i++) {
		    	
		    	 System.out.println(n*i);;
		    	 
		    	 try {
		    		    Thread.sleep(500);   
		    	 }catch(Exception e) { }
		     }
	   }
	
}




public class staticsybnexample {

	    public static void main(String args[] ) {
	    	
	    }
}
